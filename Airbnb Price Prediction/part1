    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "0wDlexJGc6Lh",
        "outputId": "dd58bbd7-8f52-431b-f99d-ecbd8fae829c"
      },
      "source": [
        "# TRAINING DATA\n",
        "\n",
        "\n",
        "from sklearn.model_selection import train_test_split\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.1, random_state=15, stratify=y)\n",
        "\n",
        "from imblearn.over_sampling import SMOTE\n",
        "\n",
        "smote = SMOTE(sampling_strategy='minority')\n",
        "X_sm, y_sm = smote.fit_sample(X_train, y_train)\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "from sklearn.model_selection import train_test_split\n",
        "X_train, X_test, y_train, y_test = train_test_split(X_sm, y_sm, random_state=15)\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "RTC = RandomForestClassifier(random_state=0)\n",
        "RTC.fit(X_train,y_train)\n",
        "y_pred = RTC.predict(X_test)\n",
        "print(y_pred)\n",
        "\n",
